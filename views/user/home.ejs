</head>

<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Header Section Begin -->
    <header class="header-section">
        <div class="container">
            <div class="inner-header">
                <div class="row">
                    <div class="col-lg-2 col-md-2">
                        <div class="logo">
                            <a href="/home">
                                <h2 style="color: #e7ab3c; font-family: system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;" class="fw-bolder">ZOKSO</h2>
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-7 col-md-7">
                        <div class="advanced-search">
                            <button type="button" class="category-btn">All Categories</button>
                            <form action="/shop" method="GET" class="input-group">
                                <input name="keyword" placeholder="What do you need?">
                                <button type="submit"><i class="ti-search"></i></button>
                            </form>
                        </div>
                    </div>
                    <div class="col-lg-3 text-right col-md-3">
                        <% if (user) { %>
                        <ul class="nav-right">
                            <li>
                                <span>₹<%=user.wallet.balance%></span>
                                <a href="/wallet">
                                    <i class="fa-solid fa-wallet text-dark"></i>
                                </a>
                            </li>
                            <li class="cart-icon"><a href="/cart">
                                <i class="icon_bag_alt"></i>
                                <% if (user.cart.length > 0) { %>
                                    <span><%=user.cart.length%></span>
                                <% } %>
                            </a>
                            <% if (user.cart.length !== 0) { %>
                            <div class="cart-hover">
                                <div class="select-items">
                                    <table>
                                        <tbody>
                                            <% if (user.cart[0]) { %>
                                            <tr>
                                                <td class="si-pic"><img src="<%=user.cart[0].productId.images[1]%>" alt=""></td>
                                                <td class="si-text">
                                                    <div class="product-selected">
                                                        <p>₹<%=user.cart[0].productId.price%> x <%=user.cart[0].quantity%></p>
                                                        <h6><%=user.cart[0].productId.productName%></h6>
                                                    </div>
                                                </td>
                                                <td class="si-close">
                                                    <i class="ti-close"></i>
                                                </td>
                                            </tr>
                                            <% } %>
                                            <% if (user.cart[1]) { %>
                                            <tr>
                                                <td class="si-pic"><img src="<%=user.cart[1].productId.images[1]%>" alt=""></td>
                                                <td class="si-text">
                                                    <div class="product-selected">
                                                        <p>₹<%=user.cart[1].productId.price%> x <%=user.cart[1].quantity%></p>
                                                        <h6><%=user.cart[1].productId.productName%></h6>
                                                    </div>
                                                </td>
                                                <td class="si-close">
                                                    <i class="ti-close"></i>
                                                </td>
                                            </tr>
                                            <% } %>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="select-button">
                                    <a href="/cart" class="primary-btn view-card">VIEW CARD</a>
                                    <a href="/checkout" class="primary-btn checkout-btn">CHECK OUT</a>
                                </div>
                            </div>
                            <% } %>
                            </li>
                        </ul>
                        <% } %>
                    </div>
                </div>
            </div>
        </div>
        <div class="nav-item">
            <div class="container">
                <div class="nav-depart">
                    <div class="depart-btn">
                        <i class="ti-menu"></i>
                        <span>All departments</span>
                        <ul class="depart-hover">
                            <li class="active"><a href="#">Women’s Clothing</a></li>
                            <li><a href="#">Men’s Clothing</a></li>
                            <li><a href="#">Underwear</a></li>
                            <li><a href="#">Kid's Clothing</a></li>
                            <li><a href="#">Brand Fashion</a></li>
                            <li><a href="#">Accessories/Shoes</a></li>
                            <li><a href="#">Luxury Brands</a></li>
                            <li><a href="#">Brand Outdoor Apparel</a></li>
                        </ul>
                    </div>
                </div>
                <nav class="nav-menu mobile-menu">
                    <ul>
                        <li class="active"><a href="/home">Home</a></li>
                        <li><a href="/shop">Shop</a></li>
                        <li><a href="#">Collection</a>
                            <ul class="dropdown">
                                <li><a href="#">Men's</a></li>
                                <li><a href="#">Women's</a></li>
                                <li><a href="#">Kid's</a></li>
                            </ul>
                        </li>
                        <li><a href="./contact.html">Contact</a></li>
                        <li><a href="#">Pages</a>
                            <ul class="dropdown">
                                <li><a href="./blog-details.html">Blog Details</a></li>
                                <li><a href="./shopping-cart.html">Shopping Cart</a></li>
                                <li><a href="./check-out.html">Checkout</a></li>
                                <li><a href="./faq.html">Faq</a></li>
                                <li><a href="./register.html">Register</a></li>
                                <li><a href="./login.html">Login</a></li>
                            </ul>
                        </li>
                        <% if (user) { %>
                            <li><a href="/account"> <i class="fa-regular fa-user"></i> &nbsp; Account</a></li>
                        <% } else { %>
                            <li><a href="/login"> <i class="fa-regular fa-user"></i> &nbsp; Login</a></li>
                        <% } %>
                    </ul>
                </nav>
                <div id="mobile-menu-wrap"></div>
            </div>
        </div>
    </header>
    <!-- Header End -->

    <!-- Hero Section Begin -->
    <section class="hero-section">
        <div class="hero-items owl-carousel">
            <% mainBanner.forEach(banner => { %>
            <div class="single-hero-items set-bg" data-setbg="<%=banner.image%>">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-5">
                            <span><%=banner.categoryName%></span>
                            <h1><%=banner.title%></h1>
                            <p><%=banner.description%></p>
                            <a href="<%=banner.linkTo%>" class="primary-btn">Shop Now</a>
                        </div>
                    </div>
                    <div class="off-card">
                        <h2>Upto <span><%=banner.offer%>%</span></h2>
                    </div>
                </div>
            </div>
            <% }); %>
        </div>
    </section>
    <!-- Hero Section End -->

    <!-- Banner Section Begin -->
    <div class="banner-section spad">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-4">
                    <div class="single-banner">
                        <a href="/shop/men">
                            <img src="<%=menCategory.images[0]%>" alt="">
                            <div class="inner-text">
                                <h4>Men’s</h4>
                            </div>
                        </a>
                        
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="single-banner">
                        <a href="/shop/women">
                            <img src="<%=womenCategory.images[0]%>" alt="">
                            <div class="inner-text">
                                <h4>Women’s</h4>
                            </div>
                        </a>
                        
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="single-banner">
                        <a href="/shop/kids">
                            <img src="<%=kidsCategory.images[0]%>" alt="">
                            <div class="inner-text">
                                <h4>Kid’s</h4>
                            </div>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Banner Section End -->

    <!-- Women Banner Section Begin -->
    <% if (!user || user.gender === "female") { %>
    <section class="women-banner spad">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-3">
                    <div class="product-large set-bg" data-setbg="<%=womenBanner.image%>">
                        <h2>Women’s</h2>
                        <a href="/shop/women">Discover More</a>
                    </div>
                </div>
                <div class="col-lg-8 offset-lg-1">
                    <div class="filter-control">
                        <a href="/shop/women">
                            <h4 class="fw-bold text-dark text-decoration-underline">Explore more like these</h4>
                        </a>
                    </div>
                    <div class="product-slider owl-carousel">
                        <% womenProduct.forEach(product => { %>
                        <div class="product-item">
                            <div class="pi-pic">
                                <img src="<%=product.images[1]%>" alt="">
                                <!-- <div class="sale">Sale</div> -->
                                <div class="icon">
                                    <i class="icon_heart_alt"></i>
                                </div>
                                <ul>
                                    <li class="w-icon active"><a href="#"><i class="icon_bag_alt"></i></a></li>
                                    <li class="quick-view"><a href="/product/<%=product._id%>">+ Quick View</a></li>
                                    <li class="w-icon"><a href="#"><i class="fa fa-random"></i></a></li>
                                </ul>
                            </div>
                            <div class="pi-text">
                                <div class="catagory-name"><%=product.category.categoryName%></div>
                                <a href="/product/<%=product._id%>">
                                    <h5><%=product.productName%></h5>
                                </a>
                                <div class="product-price">
                                    ₹<%=product.price-1%>
                                    <span>₹<%=product.price%></span>
                                </div>
                            </div>
                        </div>
                        <% }); %>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <% } %>
